<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
<title>ammap</title>
<link rel="stylesheet" type="text/css" href="lightMain.css" />
<script type="text/javascript">
function reloadMap(){

	var so = new SWFObject("ammap/ammap.swf", "ammap", "100%", "", "8", "#444444");
	so.addVariable("path", "ammap/");
	so.addVariable("settings_file", escape("ammap/ammap_settings.xml"));
	so.addVariable("data_file", escape("ammap/ammap_data.xml"));
	so.write("flashcontent");

	var map = document.getElementById("ammap");
	map.style.height = (window.innerHeight - 167) + "px";
	map.style.width = window.innerWidth + "px";
}
</script>
</head>
<body onLoad="ammap.focus();" onResize="reloadMap();">
        <script type="text/javascript" src="ammap/swfobject.js"></script>
        <div id="header">
                <img src="images/header_logo.png" />
                <img src="images/header_menu_event_off.png" />
                <img src="images/header_menu_separator.png" />
                <img src="images/header_menu_pray_off.png" />
                <img src="images/header_menu_separator.png" />
                <img src="images/header_menu_send_off.png" />
                <img src="images/header_menu_separator.png" />
                <img src="images/header_menu_go_off.png" />
        </div>
        <div id="flashcontent">
                <strong>Please upgrade your Flash Player to see the world map.</strong>
        </div>
        <div id="footer">
                <img src="images/footer_link_joshproj.png" />
                <img src="images/footer_link_joshproj.png" />
                <img src="images/footer_link_joshproj.png" />
                <img src="images/footer_link_joshproj.png" />
        </div>

        <script type="text/javascript">
                // <![CDATA[		
                var so = new SWFObject("ammap/ammap.swf", "ammap", "100%", "100%", "8", "#444444");
                so.addVariable("path", "ammap/");
                so.addVariable("settings_file", escape("ammap/ammap_settings.xml"));
                so.addVariable("data_file", escape("ammap/ammap_data.xml"));
                so.write("flashcontent");
                // ]]>
        </script>
<!-- end of ammap script -->
</body>
</html>
